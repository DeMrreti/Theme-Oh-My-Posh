{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#E36464",
          "style": "plain",
          "template": " {{ .UserName }} ",
          "type": "session"
        },
        {
          "foreground": "#56B6C2",
          "style": "plain",
          "template": "\u26A1",
          "type": "text"
        },
        {
          "foreground": "#56B6C2",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " {{ .Path }}",
          "type": "path"
        },
        {
          "foreground": "#62ED8B",
          "properties": {
            "style": "roundrock",
            "threshold": 0
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }}",
          "type": "executiontime"
        },
        {
          "foreground": "#62ED8B",
          "style": "diamond",
          "template": " <#ffffff>CPU:</> {{ round .PhysicalPercentUsed .Precision }}%",
          "type": "sysinfo"
        },
        {
          "foreground": "#62ED8B",
          "style": "diamond",
          "template": " <#ffffff>RAM:</> {{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1000000000.0) }}/{{ (div .PhysicalTotalMemory 1000000000.0) }}GB",
          "type": "sysinfo"
        },
        {
          "foreground": "#62ED8B",
          "style": "plain",
          "template": " \u279c",
          "type": "text"
        }
        
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
